1.
 
#include <stdio.h>
 
int main() {
    char str[] = "hello";
    int counter = 0;
    for (char *c = str; *c != '\0'; c++) {
        counter++;
    }
    printf("%d", counter);
    return 0;
}
 
---------------------------------------------------------
 
2.
 
 
#include <stdio.h>
 
int main() {
    char str[] = "hello wolrd hi";
    int counter = 0;
    for (char *c = str; *c != '\0'; c++) {
            if(*c == ' ')
            counter++;
    }
    counter++;
    printf("%d", counter);
    return 0;
}
 
