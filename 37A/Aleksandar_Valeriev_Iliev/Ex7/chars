5.
 
#include <stdio.h>
#include <string.h>
 
int main() {
    char str[] = "hello world";
    int counter = 0;
    for (char *c = str; *c != '\0'; c++) {
            if(*c=='a' || *c=='o' || *c=='e' || *c=='u' || *c=='i' || *c=='y')
        counter++;
    }
    printf("%d", counter);
    return 0;
}
 
